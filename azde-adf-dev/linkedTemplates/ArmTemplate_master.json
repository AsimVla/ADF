{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "azde-adf-dev"
		},
		"azdesourcesqldb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'azdesourcesqldb'",
			"defaultValue": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=azde-source.database.windows.net;Initial Catalog=azde-sqldb-src;User ID=azde_admin"
		},
		"RestService_source_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://airlabs.co/api/v9/airlines"
		},
		"azdedatalakesink_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azdedatalakesink.dfs.core.windows.net/"
		},
		"azdedatalakesink_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "86766fcb-dccb-4d3f-bf03-469c8fe2ad9d"
		},
		"azdedatalakesink_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "71b4ee5a-5534-4972-9549-46165fae4cac"
		},
		"azdekvdev_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://azde-kv-dev.vault.azure.net/"
		},
		"azdesourcedatalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azdesourcedatalake.dfs.core.windows.net/"
		},
		"azdesourcedatalake_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "86766fcb-dccb-4d3f-bf03-469c8fe2ad9d"
		},
		"azdesourcedatalake_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "71b4ee5a-5534-4972-9549-46165fae4cac"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "azde-adf-dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"azdesourcesqldb_connectionString": {
						"value": "[parameters('azdesourcesqldb_connectionString')]"
					},
					"RestService_source_properties_typeProperties_url": {
						"value": "[parameters('RestService_source_properties_typeProperties_url')]"
					},
					"azdekvdev_properties_typeProperties_baseUrl": {
						"value": "[parameters('azdekvdev_properties_typeProperties_baseUrl')]"
					},
					"azdedatalakesink_properties_typeProperties_url": {
						"value": "[parameters('azdedatalakesink_properties_typeProperties_url')]"
					},
					"azdedatalakesink_properties_typeProperties_tenant": {
						"value": "[parameters('azdedatalakesink_properties_typeProperties_tenant')]"
					},
					"azdedatalakesink_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('azdedatalakesink_properties_typeProperties_servicePrincipalId')]"
					},
					"azdesourcedatalake_properties_typeProperties_url": {
						"value": "[parameters('azdesourcedatalake_properties_typeProperties_url')]"
					},
					"azdesourcedatalake_properties_typeProperties_tenant": {
						"value": "[parameters('azdesourcedatalake_properties_typeProperties_tenant')]"
					},
					"azdesourcedatalake_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('azdesourcedatalake_properties_typeProperties_servicePrincipalId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}